
run = "python app.py"
modules = ["python-3.10:v18-20230807-322e88b"]
hidden = [".pythonlibs", ".config"]

[nix]
channel = "stable-23_05"

[deployment]
run = ["sh", "-c", "python app.py"]
deploymentTarget = "cloudrun"
ignorePorts = false

[env]
REPLIT_DISABLE_PACKAGE_LAYER = "1"
PYTHONPATH = "${REPL_HOME}"

[[ports]]
localPort = 5000
externalPort = 80
